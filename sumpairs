
public class sumpairs {
    public static void main(String[] args) {
        int[] arr = new int[] { 1, 3, 5, 7, 9 };

        int sum = 8;
        for (int i = 0; i < arr.length; i++) {
            for (int j = 0; j < i; j++) {
                if (arr[i] + arr[j] == sum) {

                    System.out.println(arr[i] + " " + arr[j]);

                }
            }
        }

    }
}
